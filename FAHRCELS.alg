algoritmo "Celsius para Fahrenheit"
// Função :
// Autor :
// Data : 17/02/2022
// Seção de Declarações 
var
   C,F: REAL
inicio
      ESCREVA ("QUAL É A TEMPERATURA EM FAHRENHEIT? ")
      LEIA (F)
      C <- ((F-32)*0.5556)
      ESCREVA (" A TEMPERATURA EM CELSIUS É DE: ",C:4:1,"C°")
fimalgoritmo
