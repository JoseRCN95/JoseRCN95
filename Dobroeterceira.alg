algoritmo "semnome"
// Função :
// Autor :
// Data : 15/02/2022
// Seção de Declarações 
var
  V: REAL
  Vx2: real
  Vd3: real
inicio
    Escreva ("Qual é o dobro e a terça parte deste número: ")
    Leia (V)
    Vx2 <-(V*2)
    Vd3 <-(V/3)
    Escreval ("O dobro de", V, " é ", Vx2)
    Escreva ("A terceira parte de ",V, " é ", Vd3)
fimalgoritmo
